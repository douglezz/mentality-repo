<addon id="plugin.audio.CatlaEmi"
       name="CatlaEmi"
       version="0.2.3"
       provider-name="Mentality">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.beautifulsoup" version="3.2.0"/>
        <import addon="script.module.simple.downloader" version="1.9.4"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
        library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Mentality stellt vor</summary>
        <description lang="tr">23 yeni Diskografi ve 300ü aşkın sanatçı eklendi.
Fare ile sag tikla, download mp3 yada download album. Evet artik indirebiliyorsunuz :-) !!!
~~~Mentality wuenscht euch viel Spass~~~</description>
        <description lang="de">23 neue Diskogrphien und über 300 neue Sänger hinzugefügt.
Mit der Maus rechts klicken, download mp3 oder download album. Japp jetzt könnt ihr auch downloaden :-) !!!
~~~Mentality wünscht euch viel Spass~~~</description>
        <description lang="en">23 new discogrphys and over 300 new artists were added.
~~~Have fun~~~</description>
        <platform>all</platform>
    </extension>
</addon>

